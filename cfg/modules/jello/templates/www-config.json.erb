{
	"env": "<%= scope.lookupvar("::environment") %>",

	"bundleJs": <%= scope.lookupvar("jello::params::bundleJs") %>,
	"cacheViews": <%= scope.lookupvar("jello::params::cacheViews") %>,
	"debugTemplates": <%= scope.lookupvar("jello::params::debugTemplates") %>,
	"listenPort": <%= scope.lookupvar("jello::params::listenPort") %>,
	"staticBasePath": "<%= scope.lookupvar("jello::params::staticBasePath") %>",
	"scheme": "<%= scope.lookupvar("jello::params::scheme") %>",
	"useCluster": <%= scope.lookupvar("jello::params::useCluster") %>,
	"useObjectCache": <%= scope.lookupvar("jello::params::useObjectCache") %>,
	"workers": <%= scope.lookupvar("jello::params::workers") %>,

	"mail": <%= scope.function_sorted_json([ scope.lookupvar("jello::params::mail") ]) %>,

	"cache": {
		"host": "<%= scope.lookupvar("jello::params::cache_host") %>",
		"port": <%= scope.lookupvar("jello::params::cache_port") %>
	},

	"db": {
		"host": "<%= scope.lookupvar("jello::params::db_host") %>",
		"port": <%= scope.lookupvar("jello::params::db_port") %>,
		"user": "<%= scope.lookupvar("jello::params::db_user") %>",
		"password": "<%= scope.lookupvar("jello::params::db_password") %>",
		"database": "<%= scope.lookupvar("jello::params::db_database") %>"
	},

	"log": {
		"level": "<%= scope.lookupvar("jello::params::log_level") %>",
		"showPid": <%= scope.lookupvar("jello::params::log_showPid") %>,
		"timestamps": "<%= scope.lookupvar("jello::params::log_timestamps") %>",
		"transports": [ "<%= scope.lookupvar("jello::params::log_transports").join '", "' %>" ],
		"id": "jello"
	},

	"session": {
		"secret": "<%= scope.lookupvar("jello::params::session_secret") %>",
		"ttl": <%= scope.lookupvar("jello::params::session_ttl") %>,
		"key": "sid"
	},

	"store": {
		"host": "<%= scope.lookupvar("jello::params::store_host") %>",
		"port": <%= scope.lookupvar("jello::params::store_port") %>
	}
}